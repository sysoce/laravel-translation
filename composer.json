{
    "name": "sysoce/laravel-translation",
    "description": "Provides a translation dictionary layer between a translation service and your Laravel application.",
    "keywords": [
        "sysoce",
        "laravel",
        "translation",
        "auto",
        "machine",
        "dictionary"
    ],
    "type": "library",
    "homepage": "https://github.com/sysoce/laravel-translation",
    "license": "MIT",
    "authors": [
        {
            "name": "Sysoce",
            "email": "sysoce@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "google/cloud-translate": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Sysoce\\Translation\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/migrations"
        ],
        "psr-4": {
            "Sysoce\\Translation\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sysoce\\Translation\\TranslationServiceProvider"
            ]
        }
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "^7.2"
    }
}
